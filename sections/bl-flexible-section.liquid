<!-- /sections/bl-one-coumn-section.liquid -->
{{ 'bl-styling.css' | asset_url | stylesheet_tag }}
<script src="{{ 'bl-base.js' | asset_url }}" defer="defer"></script>
<link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />

{%- liquid
  assign sectionid = section.id
  assign text_color = section.settings.text_color
  assign bg_color = section.settings.bg_color
  assign bg_overlay = section.settings.opacity | divided_by: 100.00
-%}

{%- style -%}
  #bl-two-column-section--{{ sectionid }}{
    {%- if bg_color != 'rgba(0,0,0,0)' and bg_color != '' -%}
      background-color: {{ bg_color }};
    {%- endif -%}
  }

  {% if section.settings.bg_image != blank %}
    #bl-two-column-section--{{ sectionid }}{
        min-height: 100%;
        background: linear-gradient(0deg, {{ section.settings.overlay_color | color_modify: 'alpha', bg_overlay }}, {{ section.settings.overlay_color | color_modify: 'alpha', bg_overlay }}), url('{{ section.settings.bg_image | img_url: '2048x' }}');
        background-size: {{ section.settings.bg_image_size }};
        background-repeat: no-repeat;
    }
  {% endif %}

  .bl-blocks--{{ sectionid }} {
    display: grid;
    grid-template-columns: 1fr;
    justify-content: center;
    gap: {{ section.settings.gap }}px;
  }

  @media only screen and (min-width: 768px) {
    .bl-blocks-wrapper--{{ sectionid }} {
      display: flex;
      justify-content: center;
      align-items: {{ section.settings.mb_vertical }};
    }

    .bl-blocks--{{ sectionid }} {
      width: 100%;
    }

    .bl-blocks-wrapper--{{ sectionid }} {
      justify-content: {{ section.settings.horizontal }};
      align-items: {{ section.settings.vertical }};
    }

    .bl-blocks--{{ sectionid }} {   
      grid-template-columns: 
        {% if section.settings.first_column == blank %} {{ section.settings.column_ratios }}{% endif %}
        {% if section.settings.first_column != blank %} {{ section.settings.first_column }}{% endif %}
        {% if section.settings.second_column != blank %} {{ section.settings.second_column }}{% endif %}
        {% if section.settings.third_column != blank %} {{ section.settings.third_column }}{% endif %}
        {% if section.settings.fourth_column != blank %} {{ section.settings.fourth_column }}{% endif %}
        ;
      align-items: {{ section.settings.alignment }};
      width: {{ section.settings.wrapper }}%;
    }
  }

  .bl-section-padding--{{ sectionid }} {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
      padding-left: {{ section.settings.padding_left }}px;
      padding-right: {{ section.settings.padding_right }}px;"
  }

  @media only screen and (max-width: 768px) {
    .bl-section-padding--{{ sectionid }} {
      padding-top: {{ section.settings.mb_padding_top }}px;
      padding-bottom: {{ section.settings.mb_padding_bottom }}px;
      padding-left: {{ section.settings.mb_padding_left }}px;
      padding-right: {{ section.settings.mb_padding_right }}px;"
    }
  }

  .bl-blocks--{{ sectionid }} p {
    margin: 0px !important;
  }

  /* COLUMN ORDER */
  @media only screen and ( max-width: 768px ) {  
    .bl-blocks--{{ sectionid }} .bl--block:nth-child(2) { 
      order: {{ section.settings.first_order }}; 
    }
    .bl-blocks--{{ sectionid }} .bl--block:nth-child(4) { 
      order: {{ section.settings.second_order }}; 
    }
    .bl-blocks--{{ sectionid }} .bl--block:nth-child(6) { 
      order: {{ section.settings.third_order }}; 
    }
    .bl-blocks--{{ sectionid }} .bl--block:nth-child(8) { 
      order: {{ section.settings.fourth_order }}; 
    }
  }

  /* ----- BUILDING BLOCKS CSS ----- */
/* DESKTOP SECTION HEIGHT */
@media only screen and (min-width: 768px){
  .height--150 {
    height: 150px;
  }

  .height--250 {
    height: 250px;
  }

  .height--350 {
    height: 350px;
  }

  .height--450 {
    height: 450px;
  }

  .height--550 {
    height: 550px;
  }

  .height--650 {
    height: 650px;
  }

  .height--full {
    height: 100vh;
  }

  .height--1-3 {
    height: 33vh;
  }

  .height--1-2 {
    height: 50vh;
  }

  .height--2-3 {
    height: 66vh;
  }

  .height--3-4 {
    height: 75vh;
  }
}

@media only screen and (max-width: 768px){
  .mb-height--150 {
    height: 150px;
  }

  .mb-height--250 {
    height: 250px;
  }

  .mb-height--350 {
    height: 350px;
  }

  .mb-height--450 {
    height: 450px;
  }

  .mb-height--550 {
    height: 550px;
  }

  .mb-height--650 {
    height: 650px;
  }

  .mb-height--full {
    height: 100vh;
  }

  .mb-height--1-3 {
    height: 33vh;
  }

  .mb-height--1-2 {
    height: 50vh;
  }

  .mb-height--2-3 {
    height: 66vh;
  }

  .mb-height--3-4 {
    height: 75vh;
  }
}

  /* SPACE BLOCK */
  .bl-dt-space {
    display: block !important;
  }
  .bl-mb-space {
    display: none !important;
  }
  @media only screen and (max-width: 768px) {
    .bl-dt-space {
      display: none !important;
    }
    .bl-mb-space {
      display: block !important;
    }
  }

  /* BUTTON */
  .bl-btn {
    display: inline-block;
    text-decoration: none;
    transition-duration: 0.4s;
  }

  .bl-btn:hover {
    opacity: 0.7;
  }

  /* BUTTON STYLES */
  .bl-btn--full-w-tall {
    text-align: center !important;
    padding: 12px 12px;
    width: 100%;
  }
  .bl-btn--full-w-medium {
    text-align: center !important;
    padding: 6px 6px;
    width: 100%;
  }
  .bl-btn--full-w-short {
    text-align: center !important;
    padding: 0px 4px;
    width: 100%;
  }

  .bl-btn--xl {
    padding: 12px 32px;
  }
  @media only screen and (max-width: 768px) {
    .bl-btn--xl {
      padding: 10px 28px;
    }
  }
  .bl-btn--lg {
    padding: 10px 28px;
  }
  @media only screen and (max-width: 768px) {
    .bl-btn--lg {
      padding: 8px 24px;
    }
  }
  .bl-btn--md {
    padding: 8px 24px;
  }
  @media only screen and (max-width: 768px) {
    .bl-btn--md {
      padding: 6px 20px;
    }
  }
  .bl-btn--sm {
    padding: 6px 20px;
  }
  @media only screen and (max-width: 768px) {
    .bl-btn--sm {
      padding: 4px 16px;
    }
  }
  .bl-btn--xs {
    padding: 4px 16px;
  }
  @media only screen and (max-width: 768px) {
    .bl-btn--xs {
      padding: 2px 12px;
    }
  }

  /* DISPLAY SETTINGS */
  /* .bl-hide-none {} */
  @media only screen and (min-width: 768px) {
    .bl-hide-desktop {
      display: none !important;
    }
  }
  @media only screen and (max-width: 768px) {
    .bl-hide-mobile {
      display: none !important;
    }
  }

{%- endstyle -%}

<section
  id="bl-two-column-section--{{ sectionid }}"
  class="bl-section-padding--{{ sectionid }} {{ section.settings.display_options }}">
  <div class="bl-img-overlay">
      <div style="max-width: {{ section.settings.width }}px;" class="bl-blocks-wrapper--{{ sectionid }} {{ section.settings.dt_height }} {{ section.settings.mb_height }} bl-m-auto bl-px-4">
        <div class="bl-blocks--{{ sectionid }}">

          {% for block in section.blocks %}
            {% case block.type %}

              {% when '@app' %}
                <div class="bl--block-wrapper--{{ block.id }}">
                    {% render block %}
                </div>

              {% comment %} TEXT BLOCK {% endcomment %}
              {% when 'text' %}
                {%- liquid
                  assign column_image_overlay = block.settings.opacity | divided_by: 100.00
                  assign blockid = block.id
                -%}

                {%- style -%}
                .bl--block-wrapper--{{ blockid }} {
                  padding: {{ block.settings.dt_column_padding }}px;
                }
                .bl--block-wrapper--{{ block.id }} .bl-subheading {
                  font-size: {{ block.settings.dt_subheading_font_size }}px;
                }
                .bl--block-wrapper--{{ block.id }} .bl-heading {
                  font-size: {{ block.settings.dt_heading_font_size }}px;
                }
                .bl--block-wrapper--{{ block.id }} .bl-description {
                  font-size: {{ block.settings.dt_description_font_size }}px;
                }
                .bl--block-wrapper--{{ block.id }} .bl-btn {
                  font-size: {{ block.settings.dt_btn_font_size }}px;
                }
                .bl--block-wrapper--{{ block.id }} .bl-under-text {
                  font-size: {{ block.settings.dt_under_text_font_size }}px;
                }


                @media only screen and (max-width: 768px) {
                  .bl--block-wrapper--{{ blockid }} {
                    padding: {{ block.settings.mb_column_padding }}px;
                  }
                  .bl--block-wrapper--{{ block.id }} .bl-subheading {
                    font-size: {{ block.settings.mb_subheading_font_size }}px;
                  }
                  .bl--block-wrapper--{{ block.id }} .bl-heading {
                    font-size: {{ block.settings.mb_heading_font_size }}px;
                  }
                  .bl--block-wrapper--{{ block.id }} .bl-description {
                    font-size: {{ block.settings.mb_description_font_size }}px;
                  }
                  .bl--block-wrapper--{{ block.id }} .bl-btn {
                    font-size: {{ block.settings.mb_btn_font_size }}px;
                  }
                  .bl--block-wrapper--{{ block.id }} .bl-under-text {
                    font-size: {{ block.settings.mb_under_text_font_size }}px;
                  }
                }
                {%- endstyle -%}
                <div class="bl-text-container bl--block {{ block.settings.display_options }}">
                <div class="bl--block-wrapper--{{ blockid }}" style="text-align: {{ block.settings.text_align }}; border-style: solid; border-width: {{ block.settings.border_width }}px; border-radius: {{ block.settings.border_radius }}px; border-color: {{ block.settings.border_color }}; background-color: {{ block.settings.bg_color }}; {% if block.settings.bg_image != blank %} background: linear-gradient(0deg, {{ block.settings.overlay_color | color_modify: 'alpha', column_image_overlay }}, {{ block.settings.overlay_color | color_modify: 'alpha', column_image_overlay }}), url('{{ block.settings.bg_image | img_url: '1024x' }}'); min-height: 100%; background-size: {{ block.settings.bg_image_size }}; background-repeat: no-repeat; background-position-x: {{ block.settings.image_alignment_x }}; background-position-y: {{ block.settings.image_alignment_y }}; {% endif %}">
                  {% if block.settings.subheading != blank %}
                    <h5 class="bl-subheading" style="color: {{ block.settings.subheading_color }}; letter-spacing: {{ block.settings.subheading_letter }}px; margin: 0px 0px {{ block.settings.subheading_space }}px 0px;">{{ block.settings.subheading }}</h4>
                  {% endif %}

                  {% if block.settings.heading != blank %}
                    <h2 class="bl-heading" style="color: {{ block.settings.heading_color }}; margin: 0px 0px  {{ block.settings.heading_space }}px 0px;">{{ block.settings.heading }}</h2>
                  {% endif %}

                  {% if block.settings.description != blank %}
                    <div class="bl-description" style="color: {{ block.settings.description_color }};  margin: 0px 0px {{ block.settings.description_space }}px 0px;">{{ block.settings.description }}</div>
                  {% endif %}

                  {% if block.settings.btn_text != blank %}
                    <a class="bl-btn {{ block.settings.btn_size }}" href="{{ block.settings.btn_link }}" style="color: {{ block.settings.btn_color }}; background-color: {{ block.settings.btn_bg_color }}; border-style: solid; border-width: {{ block.settings.btn_border_width }}px; border-radius: {{ block.settings.btn_border_radius }}px; border-color: {{ block.settings.btn_border_color }};  margin: 0px 0px {{ block.settings.btn_space }}px 0px; {% if block.settings.underline_show %} text-decoration: underline; {% endif %}">{{ block.settings.btn_text }}</a>
                  {% endif %}

                  {% if block.settings.under_text != blank %}
                    <div class="bl-under-text" style="color: {{ block.settings.under_text_color }};  margin: 0px 0px {{ block.settings.under_text_space }}px 0px;">{{ block.settings.under_text }}</div>
                  {% endif %}
                </div>
              </div>

              {% comment %} ACCORDION BLOCK {% endcomment %}
              {% when 'accordion' %}
                {%- liquid
                  assign column_image_overlay = block.settings.opacity | divided_by: 100.00
                  assign blockid = block.id
                -%}

                {%- style -%}
                .bl--block-wrapper--{{ blockid }} {
                  padding: {{ block.settings.dt_column_padding }}px;
                }
                .bl--block-wrapper--{{ block.id }} .bl-subheading {
                  font-size: {{ block.settings.dt_subheading_font_size }}px;
                }
                .bl--block-wrapper--{{ block.id }} .bl-heading {
                  font-size: {{ block.settings.dt_heading_font_size }}px;
                }
                .bl--block-wrapper--{{ block.id }} .bl-description {
                  font-size: {{ block.settings.dt_description_font_size }}px;
                }
                .bl--block-wrapper--{{ block.id }} .bl-accordion {
                  font-size: {{ block.settings.dt_accordion_font_size }}px;
                }
                .bl--block-wrapper--{{ block.id }} .bl-panel {
                  font-size: {{ block.settings.dt_panel_font_size }}px;
                }
                .bl-accordion:after {
                  font-size: {{ block.settings.dt_accordion_font_size }}px;
                }
                .active:after {
                  font-size: {{ block.settings.dt_accordion_font_size }}px;
                }


                @media only screen and (max-width: 768px) {
                  .bl--block-wrapper--{{ blockid }} {
                    padding: {{ block.settings.mb_column_padding }}px;
                  }
                  .bl--block-wrapper--{{ block.id }} .bl-subheading {
                    font-size: {{ block.settings.mb_subheading_font_size }}px;
                  }
                  .bl--block-wrapper--{{ block.id }} .bl-heading {
                    font-size: {{ block.settings.mb_heading_font_size }}px;
                  }
                  .bl--block-wrapper--{{ block.id }} .bl-description {
                    font-size: {{ block.settings.mb_description_font_size }}px;
                  }
                  .bl--block-wrapper--{{ block.id }} .bl-accordion {
                    font-size: {{ block.settings.mb_accordion_font_size }}px;
                  }
                  .bl--block-wrapper--{{ block.id }} .bl-panel {
                    font-size: {{ block.settings.mb_panel_font_size }}px;
                  }
                  .bl-accordion:after {
                    font-size: {{ block.settings.mb_accordion_font_size }}px;
                  }
                  .active:after {
                    font-size: {{ block.settings.mb_accordion_font_size }}px;
                  }
                }

                .bl-accordion {
                  background-color: transparent;
                  cursor: pointer;
                  padding: {{ block.settings.accordion_column_padding }}px;
                  width: 100%;
                  border: none;
                  border-top: solid 1px {{ block.settings.border_accordion_color }};
                  text-align: left;
                  outline: none;
                  transition: 0.4s;
                }

                .bl-panel:last-child {
                  border-bottom: solid 1px {{ block.settings.border_accordion_color }};
                }

                .active, .bl-accordion:hover {
                  background-color: transparent;
                }

                .bl-accordion:after {
                  content: '\002B';
                  color: {{ block.settings.icon_accordion_color }};
                  font-weight: bold;
                  float: right;
                  margin-left: 5px;
                }

                .active:after {
                  content: "\2212";
                }

                .bl-panel {
                  text-align: left;
                  padding: 0px {{ block.settings.accordion_column_padding }}px;
                  max-height: 0;
                  overflow: hidden;
                  transition: max-height 0.2s ease-out;
                }

                .bl-panel p {
                  padding-bottom: {{ block.settings.accordion_column_padding }}px;
                }
                {%- endstyle -%}
                <div class="bl-text-container bl--block {{ block.settings.display_options }}">
                <div class="bl--block-wrapper--{{ blockid }}" style="text-align: {{ block.settings.text_align }}; border-style: solid; border-width: {{ block.settings.border_width }}px; border-radius: {{ block.settings.border_radius }}px; border-color: {{ block.settings.border_color }}; background-color: {{ block.settings.bg_color }}; {% if block.settings.bg_image != blank %} background: linear-gradient(0deg, {{ block.settings.overlay_color | color_modify: 'alpha', column_image_overlay }}, {{ block.settings.overlay_color | color_modify: 'alpha', column_image_overlay }}), url('{{ block.settings.bg_image | img_url: '1024x' }}'); min-height: 100%; background-size: {{ block.settings.bg_image_size }}; background-repeat: no-repeat; background-position-x: {{ block.settings.image_alignment_x }}; background-position-y: {{ block.settings.image_alignment_y }}; {% endif %}">
                  {% if block.settings.subheading != blank %}
                    <h5 class="bl-subheading" style="color: {{ block.settings.subheading_color }}; letter-spacing: {{ block.settings.subheading_letter }}px; margin: 0px 0px {{ block.settings.subheading_space }}px 0px;">{{ block.settings.subheading }}</h4>
                  {% endif %}

                  {% if block.settings.heading != blank %}
                    <h2 class="bl-heading" style="color: {{ block.settings.heading_color }}; margin: 0px 0px  {{ block.settings.heading_space }}px 0px;">{{ block.settings.heading }}</h2>
                  {% endif %}

                  {% if block.settings.description != blank %}
                    <div class="bl-description" style="color: {{ block.settings.description_color }};  margin: 0px 0px {{ block.settings.description_space }}px 0px;">{{ block.settings.description }}</div>
                  {% endif %}

                  {% comment %} ACCORDION 1 {% endcomment %}
                  {% if block.settings.accordion_1_text != blank %}
                    <button class="bl-accordion" style="color: {{ block.settings.accordion_color }};">{{ block.settings.accordion_1_text }}</button>
                  {% endif %}

                  {% if block.settings.panel_1_text != blank %}
                    <div class="bl-panel" style="color: {{ block.settings.panel_color }};">
                      {{ block.settings.panel_1_text }}
                    </div>
                  {% endif %}

                  {% comment %} ACCORDION 2 {% endcomment %}
                  {% if block.settings.accordion_2_text != blank %}
                    <button class="bl-accordion" style="color: {{ block.settings.accordion_color }};">{{ block.settings.accordion_2_text }}</button>
                  {% endif %}

                  {% if block.settings.panel_2_text != blank %}
                    <div class="bl-panel" style="color: {{ block.settings.panel_color }};">
                      {{ block.settings.panel_2_text }}
                    </div>
                  {% endif %}

                  {% comment %} ACCORDION 3 {% endcomment %}
                  {% if block.settings.accordion_3_text != blank %}
                    <button class="bl-accordion" style="color: {{ block.settings.accordion_color }};">{{ block.settings.accordion_3_text }}</button>
                  {% endif %}

                  {% if block.settings.panel_3_text != blank %}
                    <div class="bl-panel" style="color: {{ block.settings.panel_color }};">
                      {{ block.settings.panel_3_text }}
                    </div>
                  {% endif %}

                  {% comment %} ACCORDION 4 {% endcomment %}
                  {% if block.settings.accordion_4_text != blank %}
                    <button class="bl-accordion" style="color: {{ block.settings.accordion_color }};">{{ block.settings.accordion_4_text }}</button>
                  {% endif %}

                  {% if block.settings.panel_4_text != blank %}
                    <div class="bl-panel" style="color: {{ block.settings.panel_color }};">
                      {{ block.settings.panel_4_text }}
                    </div>
                  {% endif %}

                  {% comment %} ACCORDION 5 {% endcomment %}
                  {% if block.settings.accordion_5_text != blank %}
                    <button class="bl-accordion" style="color: {{ block.settings.accordion_color }};">{{ block.settings.accordion_5_text }}</button>
                  {% endif %}

                  {% if block.settings.panel_5_text != blank %}
                    <div class="bl-panel" style="color: {{ block.settings.panel_color }};">
                      {{ block.settings.panel_5_text }}
                    </div>
                  {% endif %}

                  {% comment %} ACCORDION 6 {% endcomment %}
                  {% if block.settings.accordion_6_text != blank %}
                    <button class="bl-accordion" style="color: {{ block.settings.accordion_color }};">{{ block.settings.accordion_6_text }}</button>
                  {% endif %}

                  {% if block.settings.panel_6_text != blank %}
                    <div class="bl-panel" style="color: {{ block.settings.panel_color }};">
                      {{ block.settings.panel_6_text }}
                    </div>
                  {% endif %}
                </div>
              </div>

              {% comment %} STAT BLOCK {% endcomment %}
              {% when 'stats' %}
                {%- liquid
                  assign column_image_overlay = block.settings.opacity | divided_by: 100.00
                  assign blockid = block.id
                -%}

                {%- style -%}
                .bl--block-wrapper--{{ blockid }} {
                  padding: {{ block.settings.dt_column_padding }}px;
                }
                .bl--block-wrapper--{{ block.id }} .bl-subheading {
                  font-size: {{ block.settings.dt_subheading_font_size }}px;
                }
                .bl--block-wrapper--{{ block.id }} .bl-heading {
                  font-size: {{ block.settings.dt_heading_font_size }}px;
                }
                .bl--block-wrapper--{{ block.id }} .bl-description {
                  font-size: {{ block.settings.dt_description_font_size }}px;
                }

                .bl--block-wrapper--{{ block.id }} .bl-stat {
                  font-size: {{ block.settings.dt_stat_font_size }}px;
                }
                .bl--block-wrapper--{{ block.id }} .bl-unit{
                  font-size: {{ block.settings.dt_unit_font_size }}px;
                }


                @media only screen and (max-width: 768px) {
                  .bl--block-wrapper--{{ blockid }} {
                    padding: {{ block.settings.mb_column_padding }}px;
                  }
                  .bl--block-wrapper--{{ block.id }} .bl-subheading {
                    font-size: {{ block.settings.mb_subheading_font_size }}px;
                  }
                  .bl--block-wrapper--{{ block.id }} .bl-heading {
                    font-size: {{ block.settings.mb_heading_font_size }}px;
                  }
                  .bl--block-wrapper--{{ block.id }} .bl-description {
                    font-size: {{ block.settings.mb_description_font_size }}px;
                  }

                  .bl--block-wrapper--{{ block.id }} .bl-stat {
                    font-size: {{ block.settings.mb_stat_font_size }}px;
                  }
                  .bl--block-wrapper--{{ block.id }} .bl-unit{
                    font-size: {{ block.settings.mb_unit_font_size }}px;
                  }
                }
                {%- endstyle -%}
                {% capture stats_area %}
                  <div class="bl-stats--{{ blockid }}" style="display: grid; grid-template-{{ block.settings.stats_direction }}: 1fr {% if block.settings.stat_2 != blank %}1fr {% endif %}{% if block.settings.stat_3 != blank %}1fr{% endif %}; gap: {{ block.settings.stats_gap }}px; line-height: normal;">
                    <div class="bl-stats-and-units"><span class="bl-stat" style="color: {{ block.settings.stat_color }};">{% if block.settings.stat_1 != blank %}{{ block.settings.stat_1 }}{% endif %}</span><span class="bl-unit" style="margin-left: 6px; color: {{ block.settings.unit_color }};">{% if block.settings.unit_1 != blank %}{{ block.settings.unit_1 }}{% endif %}</span></div>
                    <div class="bl-stats-and-units"><span class="bl-stat" style="color: {{ block.settings.stat_color }};">{% if block.settings.stat_2 != blank %}{{ block.settings.stat_2 }}{% endif %}</span><span class="bl-unit" style="margin-left: 6px; color: {{ block.settings.unit_color }};">{% if block.settings.unit_2 != blank %}{{ block.settings.unit_2 }}{% endif %}</span></div>
                    <div class="bl-stats-and-units"><span class="bl-stat" style="color: {{ block.settings.stat_color }};">{% if block.settings.stat_3 != blank %}{{ block.settings.stat_3 }}{% endif %}</span><span class="bl-unit" style="margin-left: 6px; color: {{ block.settings.unit_color }};">{% if block.settings.unit_3 != blank %}{{ block.settings.unit_3 }}{% endif %}</span></div>
                  </div>
