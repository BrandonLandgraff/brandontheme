{%- style -%}
/* PRIMARY BUTTON STYLE */
.bl-btn-primary {
  background-color: {{ buttonbackgroundcolor1 }};
  color: {{ buttoncolor1 }};
  border-width: {{ buttonborderwidth1 }}px;
  border-style: solid;
  border-color: {{ buttonbordercolor1 }};
  font-size: {{ buttonfontsize1 }}px;
  border-radius: {{ buttoncornerradius1 }}px;
}

@media only screen and (max-width: 768px) {
  .bl-btn-primary {
    font-size: {{ buttonmbfontsize1 }}px;
  }
}

.bl-btn-primary:hover {
  background-color: {{ buttonbackgroundhovercolor1 }};
  color: {{ buttonhovercolor1 }};
  border-color: {{ buttonborderhovercolor1 }};
}

/* PRIMARY LINK STYLE */
.bl-link-primary {
  color: {{ linkcolor1 }};
  text-decoration-thickness: {{ linkborderwidth1 }}px;
  text-decoration-line: underline;
  text-decoration-color: {{ linkbordercolor1 }};
  font-size: {{ linkfontsize1 }}px;
}

@media only screen and (max-width: 768px) {
  .bl-link-primary {
    font-size: {{ linkmbfontsize1 }}px;
  }
}

.bl-link-primary:hover {
  color: {{ linkhovercolor1 }};
  border-color: {{ linkborderhovercolor1 }};
}

/* SECONDARY BUTTON STYLE */
.bl-btn-secondary {
  background-color: {{ buttonbackgroundcolor2 }};
  color: {{ buttoncolor2 }};
  border-width: {{ buttonborderwidth2 }}px;
  border-style: solid;
  border-color: {{ buttonbordercolor2 }};
  font-size: {{ buttonfontsize2 }}px;
  border-radius: {{ buttoncornerradius2 }}px;
}

@media only screen and (max-width: 768px) {
  .bl-btn-secondary {
    font-size: {{ buttonmbfontsize2 }}px;
  }
}

.bl-btn-secondary:hover {
  background-color: {{ buttonbackgroundhovercolor2 }};
  color: {{ buttonhovercolor2 }};
  border-color: {{ buttonborderhovercolor2 }};
}

/* SECONDARY LINK STYLE */
.bl-link-secondary {
  color: {{ linkcolor2 }};
  text-decoration-thickness: {{ linkborderwidth2 }}px;
  text-decoration-line: underline;
  text-decoration-color: {{ linkbordercolor2 }};
  font-size: {{ linkfontsize2 }}px;
}

@media only screen and (max-width: 768px) {
  .bl-link-secondary {
    font-size: {{ linkmbfontsize2 }}px;
  }
}

.bl-link-secondary:hover {
  color: {{ linkhovercolor2 }};
  text-decoration-color: {{ linkborderhovercolor2 }};
}

