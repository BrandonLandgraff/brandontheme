<section
  id="bl-flexi-column-section--{{ sectionid }}"
  class="bl-section-padding--{{ sectionid }} {{ section.settings.display_options }}">
  <div class="bl-img-overlay">
      <div style="max-width: {% if section.settings.full_width %}100%{% else %}{{ section.settings.width }}px;{% endif %}" class="bl-blocks-wrapper--{{ sectionid }} {{ section.settings.dt_height }} {{ section.settings.mb_height }} bl-m-auto">
        <div class="bl-blocks--{{ sectionid }}">

          {% for block in section.blocks %}
            {% case block.type %}

              {% when '@app' %}
                <div class="bl--block-wrapper--{{ block.id }}">
                    {% render block %}
                </div>

              {% comment %} TEXT BLOCK {% endcomment %}
              {% when 'text' %}
                {%- liquid
                  assign column_image_overlay = block.settings.opacity | divided_by: 100.00
                  assign show_col_bg = block.settings.show_col_bg
                  assign blockid = block.id
                  assign style_id = block.settings.styling_identification
                -%}

                {% assign json_data = shop.metafields.flexi.flexistyles.value %}
                {% assign value = json_data.allstyles['{{ style_id }}'] %}

                {%- style -%}
                .bl--block-wrapper--{{ blockid }} {
                  padding: {{ block.settings.dt_column_padding }}px;
                }
                @media only screen and (max-width: 768px) {
                  .bl--block-wrapper--{{ blockid }} {
                    padding: {{ block.settings.mb_column_padding }}px;
                  }
                }

                {{ json_data.allstyles[style_id].botResponse }}

                {%- endstyle -%}

                {% capture bg_img %}{% if block.settings.bg_image != blank %} background: linear-gradient(0deg, {{ block.settings.overlay_color | color_modify: 'alpha', column_image_overlay }}, {{ block.settings.overlay_color | color_modify: 'alpha', column_image_overlay }}), url('{{ block.settings.bg_image | img_url: '1024x' }}'); background-size: {{ block.settings.bg_image_size }}; background-repeat: no-repeat; background-position-x: {{ block.settings.image_alignment_x }}; background-position-y: {{ block.settings.image_alignment_y }}; {% endif %}{% endcapture %}

                <div class="bl-text-container bl--block {{ block.settings.display_options }}" style="align-items: {{ block.settings.alignment }}; justify-content: {{ block.settings.justify_content }}; border-radius: {{ block.settings.border_radius }}px; background-color: {{ block.settings.bg_col_color }}; {% if show_col_bg == true %}{{ bg_img }}{% endif %}">
                  <div class="bl--block-wrapper--{{ blockid }} bl-ai-styles" style="text-align: {{ block.settings.text_align }}; border-style: solid; border-width: {{ block.settings.border_width }}px; border-radius: {{ block.settings.border_radius }}px; border-color: {{ block.settings.border_color }}; background-color: {{ block.settings.bg_color }}; {% if show_col_bg == false %}{{ bg_img }}{% endif %}">
                    {% comment %} {% if block.settings.subheading != blank %}
                      <h5 class="bl-subheading bl-{{ block.settings.subheading_text }}" style="color: {{ block.settings.subheading_color }}; margin: 0px 0px {{ block.settings.subheading_space }}px 0px;">{{ block.settings.subheading }}</h5>
                    {% endif %}

                    {% if block.settings.heading != blank %}
                      <h2 class="bl-heading" style="color: {{ block.settings.heading_color }}; margin: 0px 0px  {{ block.settings.heading_space }}px 0px;">{{ block.settings.heading }}</h2>
                    {% endif %}

                    {% if block.settings.description != blank %}
                      <p class="bl-description" style="color: {{ block.settings.description_color }};  margin: 0px 0px {{ block.settings.description_space }}px 0px; max-width: {{ block.settings.description_max_width }}px;">{{ block.settings.description }}<p>
                    {% endif %}

                    {% if block.settings.btn_text != blank %}
                      <a class="bl-btn {{ block.settings.btn_size }}  {{ block.settings.btn_style }}" style="margin: 0px 0px {{ block.settings.btn_space }}px 0px;" href="{{ block.settings.btn_link }}">{{ block.settings.btn_text }}</a>
                    {% endif %}

                    {% if block.settings.under_text != blank %}
                      <{{ block.settings.under_text }} class="bl-under-text" style="color: {{ block.settings.under_text_color }};  margin: 0px;">{{ block.settings.under }}</{{ block.settings.under_text }}>
                    {% endif %} {% endcomment %}

                    <div className="bl-ai-styles">
                      <h4 class="bl-subheading">{{ block.settings.subheading }}</h4>
                      <h2 class="bl-heading">{{ block.settings.heading }}</h2>
                      <p class="bl-description">
                        {{ block.settings.description }}
                      </p>
                    </div>

                  </div>
              </div>

              {% comment %} ACCORDION BLOCK {% endcomment %}
              {% when 'accordion' %}
                {%- liquid
                  assign column_image_overlay = block.settings.opacity | divided_by: 100.00
                  assign show_col_bg = block.settings.show_col_bg
                  assign blockid = block.id
                -%}

                {%- style -%}
                .bl--block-wrapper--{{ blockid }} {
                  padding: {{ block.settings.dt_column_padding }}px;
                }
                .bl--block-wrapper--{{ block.id }} .bl-subheading {
                  font-size: {{ block.settings.dt_subheading_font_size }}px;
                }
                .bl--block-wrapper--{{ block.id }} .bl-heading {
                  font-size: {{ block.settings.dt_heading_font_size }}px;
                }
                .bl--block-wrapper--{{ block.id }} .bl-description {
                  font-size: {{ block.settings.dt_description_font_size }}px;
                }
                .bl--block-wrapper--{{ block.id }} .bl-accordion {
                  font-size: {{ block.settings.dt_accordion_font_size }}px;
                }
                .bl--block-wrapper--{{ block.id }} .bl-panel {
                  font-size: {{ block.settings.dt_panel_font_size }}px;
                }
                .bl-accordion:after {
                  font-size: {{ block.settings.dt_accordion_font_size }}px;
                }
                .active:after {
                  font-size: {{ block.settings.dt_accordion_font_size }}px;
                }


                @media only screen and (max-width: 768px) {
                  .bl--block-wrapper--{{ blockid }} {
                    padding: {{ block.settings.mb_column_padding }}px;
                  }
                  .bl--block-wrapper--{{ block.id }} .bl-subheading {
                    font-size: {{ block.settings.mb_subheading_font_size }}px;
                  }
                  .bl--block-wrapper--{{ block.id }} .bl-heading {
                    font-size: {{ block.settings.mb_heading_font_size }}px;
                  }
                  .bl--block-wrapper--{{ block.id }} .bl-description {
                    font-size: {{ block.settings.mb_description_font_size }}px;
                  }
                  .bl--block-wrapper--{{ block.id }} .bl-accordion {
                    font-size: {{ block.settings.mb_accordion_font_size }}px;
                  }
                  .bl--block-wrapper--{{ block.id }} .bl-panel {
                    font-size: {{ block.settings.mb_panel_font_size }}px;
                  }
                  .bl-accordion:after {
                    font-size: {{ block.settings.mb_accordion_font_size }}px;
                  }
                  .active:after {
                    font-size: {{ block.settings.mb_accordion_font_size }}px;
                  }
                }

