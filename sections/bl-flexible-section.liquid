                {% capture bg_img %}{% if block.settings.bg_image != blank %} background: linear-gradient(0deg, {{ block.settings.overlay_color | color_modify: 'alpha', column_image_overlay }}, {{ block.settings.overlay_color | color_modify: 'alpha', column_image_overlay }}), url('{{ block.settings.bg_image | img_url: '1024x' }}'); background-size: {{ block.settings.bg_image_size }}; background-repeat: no-repeat; background-position-x: {{ block.settings.image_alignment_x }}; background-position-y: {{ block.settings.image_alignment_y }}; {% endif %}{% endcapture %}

                <div class="bl-text-container bl--block {{ block.settings.display_options }}" style="align-items: {{ block.settings.alignment }}; border-radius: {{ block.settings.border_radius }}px; background-color: {{ block.settings.bg_col_color }}; {% if show_col_bg == true %}{{ bg_img }}{% endif %}">
                <div class="bl--block-wrapper--{{ blockid }}" style="text-align: {{ block.settings.text_align }}; border-style: solid; border-width: {{ block.settings.border_width }}px; border-radius: {{ block.settings.border_radius }}px; border-color: {{ block.settings.border_color }}; background-color: {{ block.settings.bg_color }}; {% if show_col_bg == false %}{{ bg_img }}{% endif %}">

                  {% if block.settings.heading != blank %}
                    <h2 class="bl-heading" style="color: {{ block.settings.heading_color }};  margin: 0px 0px {{ block.settings.heading_space }}px 0px;">{{ block.settings.heading }}</h2>
                  {% endif %}

                  <div class="bl-grid" style="grid-template-columns: 1fr 1fr; gap: 12px;">
                    {% if block.settings.btn_text != blank %}
                      <a class="bl-btn {{ block.settings.btn_size }} bl-text-center" href="{{ block.settings.btn_link }}" style="color: {{ block.settings.btn_color }}; background-color: {{ block.settings.btn_bg_color }}; border-style: solid; border-width: {{ block.settings.btn_border_width }}px; border-radius: {{ block.settings.btn_border_radius }}px; border-color: {{ block.settings.btn_border_color }};  margin: 0px 0px {{ block.settings.btn_space }}px 0px; {% if block.settings.underline_show %} text-decoration: underline; {% endif %}">{{ block.settings.btn_text }}</a>
                    {% endif %}

                    {% if block.settings.btn_text != blank %}
                      <a class="bl-btn {{ block.settings.btn_alt_size }} bl-text-center" href="{{ block.settings.btn_alt_link }}" style="color: {{ block.settings.btn_alt_color }}; background-color: {{ block.settings.btn_alt_bg_color }}; border-style: solid; border-width: {{ block.settings.btn_alt_border_width }}px; border-radius: {{ block.settings.btn_alt_border_radius }}px; border-color: {{ block.settings.btn_alt_border_color }};  margin: 0px 0px {{ block.settings.btn_alt_space }}px 0px; {% if block.settings.underline_alt_show %} text-decoration: underline; {% endif %}">{{ block.settings.btn_alt_text }}</a>
                    {% endif %}
                  </div>

                  {% if block.settings.under_text != blank %}
                    <div class="bl-under-text" style="color: {{ block.settings.under_text_color }};  margin: 0px;">{{ block.settings.under_text }}</div>
                  {% endif %}
                </div>
              </div>

              {% comment %} SPACER BLOCK {% endcomment %}
              {% when 'space' %}
                {%- style -%}
                {%- endstyle -%}
                <div
                class="bl-dt-space bl--block {{ block.settings.display_options }}"
                style="
                padding-top: {{ block.settings.dt_top_space }}px;
                padding-bottom: {{ block.settings.dt_bottom_space }}px;
                padding-right: {{ block.settings.dt_right_space }}px;
                padding-left: {{ block.settings.dt_left_space }}px;
                "
                >
                {% if block.settings.show_line %}
                  <hr style="border: {{ block.settings.line_width }}px solid {{ block.settings.line_color }};">
                {% endif %}
                </div>
                <div
                class="bl-mb-space {{ block.settings.display_options }}"
                style="
                padding-top: {{ block.settings.mb_top_space }}px;
                padding-bottom: {{ block.settings.mb_bottom_space }}px;
                padding-right: {{ block.settings.mb_right_space }}px;
                padding-left: {{ block.settings.mb_left_space }}px;
                "
                >
                {% if block.settings.show_line %}
                  <hr style="border: {{ block.settings.line_width }}px solid {{ block.settings.line_color }};">
                {% endif %}
                </div>

              {% comment %} IMAGE BLOCK {% endcomment %}
              {% when 'image' %}
                {%- liquid
                  assign image_overlay = block.settings.opacity | divided_by: 100.00
                  assign blockid = block.id
                  -%}
                {%- style -%}

                .bl--image-wrapper--{{ blockid }} svg {
                  width: 100%;
                  height: 100%;
                }

                /* .bl--image-wrapper--{{ block.id }} {
                  height: {{ block.settings.desktop_img_height }};
                }
                .bl--image-wrapper--{{ block.id }} svg {
                  display: block;
                  margin: auto;
                  border-radius: {{ block.settings.img_border_radius }}px;
                  height: {{ block.settings.desktop_img_height }};
                }

                @media only screen and (max-width: 768px) {
                  .bl--image-wrapper--{{ block.id }} {
                    height: {{ block.settings.mobile_img_height }};
                  }
                  .bl--image-wrapper--{{ block.id }} svg {
                    height: {{ block.settings.mobile_img_height }};
                  }
                } */
                {%- endstyle -%}
                {% if block.settings.bg_image != blank %}
                  <div class="bl--block bl-text-container {{ block.settings.display_options }}">
                    <div class="bl--image-wrapper--{{ blockid }} {{ block.settings.desktop_img_height }} {{ block.settings.mobile_img_height }}" style="display: block; width: 100%; {% if block.settings.bg_image != blank %} background: linear-gradient(0deg, {{ block.settings.overlay_color | color_modify: 'alpha', image_overlay }}, {{ block.settings.overlay_color | color_modify: 'alpha', image_overlay }}), url('{{ block.settings.bg_image | img_url: '1024x' }}'); background-size: {{ block.settings.img_image_size }}; background-repeat: no-repeat; background-position-x: {{ block.settings.img_image_alignment_x }}; background-position-y: {{ block.settings.img_image_alignment_y }};{% endif %} border-radius: {{ block.settings.border_radius }}px;"></div>
                  </div>
                {% else %}
                  <div class="bl--block bl-text-container {{ block.settings.display_options }}">
                    <div class="bl--image-wrapper--{{ blockid }} {{ block.settings.desktop_img_height }} {{ block.settings.mobile_img_height }}" style="display: block; width: 100%; background: linear-gradient(0deg, {{ block.settings.overlay_color | color_modify: 'alpha', image_overlay }}, {{ block.settings.overlay_color | color_modify: 'alpha', image_overlay }}); background-size: {{ block.settings.img_image_size }}; background-repeat: no-repeat; background-position-x: {{ block.settings.img_image_alignment_x }}; background-position-y: {{ block.settings.img_image_alignment_y }}; border-radius: {{ block.settings.border_radius }}px; background-color: lightgray;">
                      {{ 'product-1' | placeholder_svg_tag }}
                    </div>
                  </div>
                {% endif %}
                  
            {% endcase %}
          {% endfor %}

        </div>
      </div>
  </div>
</section>

